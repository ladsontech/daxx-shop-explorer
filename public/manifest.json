
{
  "name": "Esale Uganda - Quality Gadgets, Fashion & Property",
  "short_name": "Esale UG",
  "description": "Uganda's premier online marketplace for gadgets, fashion, cosmetics, accessories, and property",
  "start_url": "/?source=pwa",
  "display": "standalone",
  "display_override": ["window-controls-overlay", "standalone", "minimal-ui"],
  "background_color": "#FFFFFF",
  "theme_color": "#2563eb",
  "orientation": "portrait-primary",
  "categories": ["shopping", "business", "lifestyle"],
  "iarc_rating_id": "e84b072d-71de-4d25-b5aa-cb41a3c26c6d",
  "scope_extensions": [
    {
      "origin": "https://esaleuganda.com"
    },
    {
      "origin": "https://www.esaleuganda.com"
    }
  ],
  "widgets": [
    {
      "name": "Quick Shop",
      "description": "Browse featured products and latest deals",
      "tag": "quick-shop",
      "template": "quick-shop-template",
      "ms_ac_template": "/widgets/quick-shop-template.html",
      "data": "/widgets/quick-shop-data.json",
      "type": "application/json",
      "screenshots": [
        {
          "src": "/widgets/quick-shop-screenshot.png",
          "sizes": "512x512",
          "label": "Quick shop widget showing featured products"
        }
      ],
      "icons": [
        {
          "src": "/images/icon.png",
          "sizes": "72x72"
        }
      ],
      "auth": false,
      "update": 3600000
    }
  ],
  "icons": [
    {
      "src": "/images/icon.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/images/icon.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/images/icon.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "maskable"
    },
    {
      "src": "/images/icon.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable"
    }
  ],
  "screenshots": [
    {
      "src": "/screenshots/home.png",
      "sizes": "1080x1920",
      "type": "image/png",
      "platform": "narrow",
      "label": "Home screen of Esale Uganda"
    }
  ],
  "shortcuts": [
    {
      "name": "Browse Gadgets",
      "url": "/gadgets?source=shortcut",
      "icons": [{ "src": "/images/icon.png", "sizes": "96x96" }]
    },
    {
      "name": "Fashion Items",
      "url": "/fashion?source=shortcut",
      "icons": [{ "src": "/images/icon.png", "sizes": "96x96" }]
    },
    {
      "name": "View Properties",
      "url": "/property?source=shortcut",
      "icons": [{ "src": "/images/icon.png", "sizes": "96x96" }]
    },
    {
      "name": "Cosmetics",
      "url": "/cosmetics?source=shortcut",
      "icons": [{ "src": "/images/icon.png", "sizes": "96x96" }]
    }
  ],
  "related_applications": [],
  "prefer_related_applications": false,
  "protocol_handlers": [
    {
      "protocol": "web+esale",
      "url": "/product/%s"
    }
  ],
  "file_handlers": [
    {
      "action": "/admin",
      "accept": {
        "text/csv": [".csv"],
        "application/json": [".json"],
        "image/jpeg": [".jpg", ".jpeg"],
        "image/png": [".png"]
      },
      "name": "Import products from file"
    }
  ],
  "share_target": {
    "action": "/share-target/",
    "method": "GET",
    "enctype": "application/x-www-form-urlencoded",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "capture_links": "existing-client-navigate",
  "scope": "/",
  "id": "esale-uganda-marketplace-pwa",
  "lang": "en",
  "dir": "ltr",
  "edge_side_panel": {
    "preferred_width": 400
  },
  "handle_links": "auto",
  "launch_handler": {
    "client_mode": ["navigate-existing", "auto"]
  }
}
